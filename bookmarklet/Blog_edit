var leng = document.getElementsByClassName('tt_article_useless_p_margin')[0].childElementCount
var result = []
for ( var i=0; i<leng; i++)
{

var idv = document.getElementsByClassName('tt_article_useless_p_margin')[0].children[i].id;
if ( idv == "custom_code_text" )
{
result+=document.getElementsByClassName('tt_article_useless_p_margin')[0].children[i].innerHTML;
}
}

function copyToClip(text) {
var textNode = document.createTextNode(text);
document.body.appendChild(textNode);

var range = document.createRange();
var selection = window.getSelection;

range.selectNodeContents(textNode);
selection().removeAllRanges();
selection().addRange(range);

document.execCommand("copy");

selection().removeAllRanges(range);
document.body.removeChild(textNode);
}

copyToClip(result);


